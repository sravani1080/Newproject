<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Text"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem6_4_docs_component_en#Text - HTL">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <multifiedwithmap
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/componentstyles"
                        name="./cq:cssClass">
                        <item jcr:primaryType="nt:unstructured">
                            <authorname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLable="Author Name"
                                name="./authorname"/>
                            <well jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <books
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}false"
                                        fieldLable="Author's Books ">
                                        <field
                                            granite:class=""
                                            jcr:primaryType="nt:unstructured"
                                            sling:resouceType="granite/ui/components/coral/foundation/form/textfied"
                                            name="./books"/>
                                    </books>
                                </items>
                            </well>
                        </item>
                    </multifiedwithmap>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
